@model CloudVOffice.Data.DTO.RetailerModel.ChangePasswordDTO;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    // Layout = "_SellerLayout";
    Layout = null;
}

<header class="main-heading">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-8 col-lg-8 col-md-8 col-sm-8">
				<div class="page-icon">
					<i class="icon-laptop_windows"></i>
				</div>
				<div class="page-title">
					<h5>Change Password</h5>
					<h6 class="sub-heading">  </h6>
				</div>
			</div>
			<div class="col-xl-4 col-lg-4 col-md-4 col-sm-4">
				<div class="right-actions">
					<span class="last-login">  </span>
				</div>
			</div>
		</div>
	</div>
</header>


<div class="main-content">
	<div class="row gutters">
		<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">

			<div class="card">

				<form asp-route="WareHouse/WareHouse/WareHouseCreate" method="post" autocomplete="off">
					<div asp-ation-summary="ModelOnly" class="message-error"></div>


					<div class="card-header"><b>Change password</b></div>
					<div class="card-body">

						<div class="form-row">

							<div class="form-group col-md-4">
								@* <input asp-for="@Model.WareHuoseId" type="hidden" /> *@

								<label for="OldPassword" class="form-label">Current Password</label>
								<div class="input-group">
									<span class="input-group-addon" id="OldPassword"><i class="icon-mobile"></i></span>
									<input id="inputEmail" asp-for="OldPassword" class="form-control" autofocus="autofocus" placeholder="Current password" />
									<span asp-validation-for="OldPassword"></span>
								</div>
							</div>


							<div class="form-group col-md-4">
								<label class="form-label" for="NewPassword">New Password</label>
								<div class="input-group">
									<span class="input-group-addon" id="password"><i class="icon-verified_user"></i></span>
									<input id="NewPassword" asp-for="NewPassword" class="form-control" autofocus="autofocus" placeholder="New password" aria-label="NewPassword" aria-describedby="password-addon" />
									<button class="btn btn-light " type="button" id="password-addon"><i class="mdi mdi-eye-outline"></i></button>
									<span asp-validation-for="NewPassword"></span>
								</div>
							</div>

							<div class="form-group col-md-4">
								<label class="form-label" for="RetypePassword">Confirm Password</label>
								<div class="input-group">
									<span class="input-group-addon" id="password"><i class="icon-verified_user"></i></span>
									<input id="RetypePassword" asp-for="RetypePassword" class="form-control" autofocus="autofocus" placeholder="Re-type new password" aria-label="RetypePassword" aria-describedby="password-addon" />
									<button class="btn btn-light " type="button" id="password-addon"><i class="mdi mdi-eye-outline"></i></button>
									<span asp-validation-for="RetypePassword"></span>
								</div>
							</div>

						</div>

						<div class="form-row">

							

						</div>

					</div>

					<div class="card-body">

						<div class="col-lg-2 col-md-2 col-sm-12">
							<button type="submit" class="btn btn-primary">Save</button>
						</div>

					</div>

				</form>

			</div>

		</div>
	</div>

</div>


