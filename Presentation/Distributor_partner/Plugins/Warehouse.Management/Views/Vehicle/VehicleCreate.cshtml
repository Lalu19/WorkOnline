@model CloudVOffice.Data.DTO.WareHouses.Vehicles.VehicleDTO;
@using System.Collections;
@using Newtonsoft.Json;
@{
	Layout = "_MainAppLayout";
	ViewData["Title"] = "Vehicle";
}
@addTagHelper *, Syncfusion.EJ2
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<style>

</style>
<header class="main-heading">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-8 col-lg-8 col-md-8 col-sm-8">
				<div class="page-icon">
					<i class="icon-laptop_windows"></i>
				</div>
				<div class="page-title">
					<h5>Ware House</h5>
					<h6 class="sub-heading">  </h6>
				</div>
			</div>
			<div class="col-xl-4 col-lg-4 col-md-4 col-sm-4">
				<div class="right-actions">
					<span class="last-login">  </span>
				</div>
			</div>
		</div>
	</div>
</header>


<div class="main-content">
	<div class="row gutters">
		<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">

			<div class="card">

				<div class="card-header">

					<div class="row">
						<div class="col-md-8">
							Vehicle
						</div>
						<div class="col-md-4 text-right float-right">
							<a href="/WareHouse/Vehicle/VehicleView" class="btn btn-success btn-sm float-right"><i class="icon-list2"></i> List </a>

						</div>
					</div>
				</div>

				<form asp-route="WareHouse/Vehicle/VehicleCreate" method="post" autocomplete="off">
					<div asp-ation-summary="ModelOnly" class="message-error"></div>


					@*<div class="card-header"><b>Pin Code</b></div>*@
					<div class="card-body">

						<div class="form-row">
							<div class="form-group col-md-4">
								<input asp-for="@Model.VehicleId" type="hidden" />

								<label for="VehicleName" class="form-label">Vehicle Name&nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.VehicleName" type="text" class="form-control" autofocus="autofocus" placeholder="Vehicle Name" />
								<span asp-validation-for="VehicleName"></span>
							</div>
							<div class="form-group col-md-4">
								<label for="VehicleNumber" class="form-label">Vehicle Number&nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.VehicleNumber" type="text" class="form-control" autofocus="autofocus" placeholder="Vehicle Number" />
								<span asp-validation-for="VehicleNumber"></span>
								<div asp-validation-summary="ModelOnly" class="message-error"></div>
							</div>
							
							<div class="form-group col-md-4">
								<label for="BrandName" class="form-label">Brand Name&nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.BrandName" type="text" class="form-control" autofocus="autofocus" placeholder="Brand Name" />
								<span asp-validation-for="BrandName"></span>
							</div>
						</div>

						<div class="form-row">
							<div class="form-group col-md-4">
								<label for="ChasisNo" class="form-label">Chasis No&nbsp;</label>
								<input asp-for="@Model.ChasisNo" type="text" class="form-control" autofocus="autofocus" placeholder="Chasis No" />
								<span asp-validation-for="ChasisNo"></span>
							</div>
							<div class="form-group col-md-4">
								<label for="FuelType" class="form-label">Fuel Type&nbsp;</label>
								<input asp-for="@Model.FuelType" type="text" class="form-control" autofocus="autofocus" placeholder="Fuel Type" />
								<span asp-validation-for="FuelType"></span>
							</div>
							<div class="form-group col-md-4">
								<label for="DL" class="form-label">DL&nbsp;</label>
								<input asp-for="@Model.DL" type="text" class="form-control" autofocus="autofocus" placeholder="DL" />
								<span asp-validation-for="DL"></span>
							</div>

						</div>


						<div class="form-row">
							<div class="form-group col-md-4">
								<label for="RC" class="form-label">RC&nbsp;</label>
								<input asp-for="@Model.RC" type="text" class="form-control" autofocus="autofocus" placeholder="RC" />
								<span asp-validation-for="RC"></span>
							</div>
							<div class="form-group col-md-4">
								<label for="InsuranceDetails" class="form-label">Insurance Number&nbsp;</label>
								<input asp-for="@Model.InsuranceDetails" type="text" class="form-control" autofocus="autofocus" placeholder="Insurance Number" />
								<span asp-validation-for="InsuranceDetails"></span>
							</div>
							<div class="form-group col-md-4">
								<label for="PurchaseYear" class="form-label">Purchase Year</label>
								<input asp-for="@Model.PurchaseYear" type="date" class="form-control" autofocus="autofocus" placeholder="Purchase Year" />
								<span asp-validation-for="PurchaseYear"></span>
							</div>

						</div>

					</div>

					<div class="card-body">

						<div class="col-lg-2 col-md-2 col-sm-12">
							<button type="submit" class="btn btn-primary">Save</button>
						</div>
					</div>

				</form>

			</div>



		</div>
	</div>

</div>