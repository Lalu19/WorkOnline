@{
	Layout = "_DistributorLayout";
	ViewData["Title"] = "Sales Order View";
}
@addTagHelper *, Syncfusion.EJ2
@using CloudVOffice.Core.Domain.Common;
@using CloudVOffice.Core.Domain.Distributor;

<header class="main-heading">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-8 col-lg-8 col-md-8 col-sm-8">
				<div class="page-icon">
					<i class="icon-laptop_windows"></i>
				</div>
				<div class="page-title">
					<h5>Distributors</h5>
					<h6 class="sub-heading">  </h6>
				</div>
			</div>
			<div class="col-xl-4 col-lg-4 col-md-4 col-sm-4">
				<div class="right-actions">
					<span class="last-login">  </span>
				</div>
			</div>
		</div>
	</div>
</header>

<div class="main-content">
	<!-- Row start -->
	<div class="row gutters">
		<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
			<div class="card">
				<div class="card-header">Order List</div>
				<div class="card-body">
					<table id="fixedHeader" class="table table-striped table-bordered">
						<thead>
							<tr>
								<th>Action</th>
								<th>Order Number</th>
								<th>Order Date</th>
							</tr>
						</thead>
						<tbody>
							@* @foreach (var item in ViewBag.OrderList)
							{
								<tr>

									<td>
										<button class="delete-button" data-orderid="@item.DPOId" style="background: transparent;border: none;">
											<i class="icon-trash" style="font-size:22px"></i>
										</button>
									</td>
									<td>@item.OrderStatus</td>
									<td>@item.DPOUniqueNo</td>
									
								</tr>
							} *@
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
	<!-- Row ends -->

</div>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
