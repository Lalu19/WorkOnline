@model CloudVOffice.Data.DTO.WareHouses.WareHouseDTO;
@using System.Collections;
@using Newtonsoft.Json;
@{
	Layout = "_MainAppLayout";
	ViewData["Title"] = "WareHouse";
}
@addTagHelper *, Syncfusion.EJ2
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<style>

</style>
<header class="main-heading">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-8 col-lg-8 col-md-8 col-sm-8">
				<div class="page-icon">
					<i class="icon-laptop_windows"></i>
				</div>
				<div class="page-title">
					<h5>Ware House</h5>
					<h6 class="sub-heading">  </h6>
				</div>
			</div>
			<div class="col-xl-4 col-lg-4 col-md-4 col-sm-4">
				<div class="right-actions">
					<span class="last-login">  </span>
				</div>
			</div>
		</div>
	</div>
</header>


<div class="main-content">
	<div class="row gutters">
		<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">

			<div class="card">

				<div class="card-header">

					<div class="row">
						<div class="col-md-8">
							Ware House
						</div>
						<div class="col-md-4 text-right float-right">
							<a href="/WareHouse/WareHouse/WareHouseView" class="btn btn-success btn-sm float-right"><i class="icon-list2"></i> List </a>

						</div>
					</div>
				</div>


				<form asp-route="WareHouse/WareHouse/WareHouseCreate" method="post" autocomplete="off">
					<div asp-ation-summary="ModelOnly" class="message-error"></div>


					<div class="card-header"><b>WareHouse Create</b></div>
					<div class="card-body">

						<div class="form-row">

							<div class="form-group col-md-4">
								<input asp-for="@Model.WareHuoseId" type="hidden" />

								<label for="WareHouseName" class="form-label">WareHouse Name&nbsp;<span style="color:red;"><b>*</b></span></label>
							<input asp-for="@Model.WareHouseName" type="text" class="form-control" autofocus="autofocus"  maxlength="11" placeholder="AAA/DDD/SS/CCC" />
								<span asp-validation-for="WareHouseName"></span>
							</div>
						

							<div class="form-group col-md-4">
								<label for="Area" class="form-label">Area Name &nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.Area" type="text" class="form-control" autofocus="autofocus" placeholder="Area Name" />
								<span asp-validation-for="Area"></span>
							</div>
							
							<div class="form-group col-md-4">
								<label for="GSTNumber" class="form-label">GST Number &nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.GSTNumber" type="text" class="form-control" autofocus="autofocus" placeholder="GST Number" />
								<span asp-validation-for="GSTNumber"></span>
							</div>

						</div>

						<div class="form-row">

							<div class="form-group col-md-4">
								<label for="Mobile" class="form-label">Mobile Number &nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.Mobile" type="text" class="form-control" autofocus="autofocus" placeholder="Mobile Number" />
								<span asp-validation-for="Mobile"></span>
							</div>

							<div class="form-group col-md-4">
								<label for="Telephone" class="form-label">Telephone &nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.Telephone" type="text" class="form-control" autofocus="autofocus" placeholder="Telephone" />
								<span asp-validation-for="Telephone"></span>
							</div>

							<div class="form-group col-md-4">
								<label for="Address" class="form-label">Address &nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.Address" type="text" class="form-control" autofocus="autofocus" placeholder="Address" />
								<span asp-validation-for="Address"></span>
							</div>
							
						</div>

						<div class="form-row">

							<div class="form-group col-md-3">
								<label for="IsActive" class="form-label">Is Active</label>
								<br>
								<input asp-for="@Model.IsActive" type="checkbox" />
								<span asp-validation-for="IsActive"></span>
							</div>

						</div>
						
					</div>

					<div class="card-body">

						<div class="col-lg-2 col-md-2 col-sm-12">
							<button type="submit" class="btn btn-primary">Save</button>
						</div>

					</div>

				</form>

			</div>

		</div>
	</div>

</div>
