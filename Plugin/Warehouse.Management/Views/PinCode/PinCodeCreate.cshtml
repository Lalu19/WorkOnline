@model CloudVOffice.Data.DTO.WareHouses.PinCodes.PinCodeDTO;
@using System.Collections;
@using Newtonsoft.Json;
@{
	Layout = "_MainAppLayout";
	ViewData["Title"] = "PinCode";
}
@addTagHelper *, Syncfusion.EJ2
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<style>

</style>
<header class="main-heading">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-8 col-lg-8 col-md-8 col-sm-8">
				<div class="page-icon">
					<i class="icon-laptop_windows"></i>
				</div>
				<div class="page-title">
					<h5>Ware House</h5>
					<h6 class="sub-heading">  </h6>
				</div>
			</div>
			<div class="col-xl-4 col-lg-4 col-md-4 col-sm-4">
				<div class="right-actions">
					<span class="last-login">  </span>
				</div>
			</div>
		</div>
	</div>
</header>


<div class="main-content">
	<div class="row gutters">
		<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">

			<div class="card">

				<div class="card-header">

					<div class="row">
						<div class="col-md-8">
							Pin Code
						</div>
						<div class="col-md-4 text-right float-right">
							<a href="/WareHouse/PinCode/PinCodeView" class="btn btn-success btn-sm float-right"><i class="icon-list2"></i> View </a>

						</div>
					</div>
				</div>

				<form asp-route="WareHouse/PinCode/PinCodeCreate" method="post" autocomplete="off">
					<div asp-ation-summary="ModelOnly" class="message-error"></div>


					@*<div class="card-header"><b>Pin Code</b></div>*@
					<div class="card-body">

						<div class="form-row">
							<div class="form-group col-md-3">
								<input asp-for="@Model.PinCodeId" type="hidden" />

								<label for="Pin" class="form-label">Pin Code&nbsp;<span style="color:red;"><b>*</b></span></label>
								<input asp-for="@Model.Pin" type="text" class="form-control" autofocus="autofocus" placeholder="Pin"/>
								<span asp-validation-for="Pin"></span>
							</div>
							<div class="form-group col-md-3">

								<label for="IsActive" class="form-label">Is Active</label>
								<br>
								<input asp-for="@Model.IsActive" type="checkbox" />
								<span asp-validation-for="IsActive"></span>
							</div>
						</div>

						<div class="form-row">
							<div class="form-group col-md-3">

								<label for="Lat" class="form-label">Latitude&nbsp;</label>
								<input asp-for="@Model.Lat" type="text" class="form-control" autofocus="autofocus" placeholder="Latitude" />
								<span asp-validation-for="Lat"></span>
							</div>
							<div class="form-group col-md-3">

								<label for="Long" class="form-label">Longitude&nbsp;</label>
								<input asp-for="@Model.Long" type="text" class="form-control" autofocus="autofocus" placeholder="Longitude" />
								<span asp-validation-for="Long"></span>
							</div>

						</div>


					</div>

					<div class="card-body">

						<div class="col-lg-2 col-md-2 col-sm-12">
							<button type="submit" class="btn btn-primary">Save</button>
						</div>
					</div>

				</form>

			</div>



		</div>
	</div>

</div>